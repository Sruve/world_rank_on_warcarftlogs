{
    "manifest_version": 3,
    "name": "Warcraftlog WR Guild Extension",
    "version": "1.0",
    "description": "Show Guild World Ranks on Warcraftlogs Rankings page",
    "permissions": [
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "https://*.warcraftlogs.com/*"
    ],
    "content_scripts": [{
        "matches": ["https://*.warcraftlogs.com/zone/rankings*"],
        "js": ["content.js"]
    }]
}